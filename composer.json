{
    "name":"daviian/converter",
    "description":"A small library to convert different bibliography formats into each other",
    "keywords":["csl", "bibtex", "endnote", "ris", "bibliography"],
    "type":"library",
    "license":"MIT",
    "homepage": "https://github.com/daviian/converter",
    "authors":[
        {
            "name":"David Schneiderbauer",
            "email":"dschneiderbauer@gmail.com"
        }
    ],
    "require":{
        "php":">=5.3.0",
        "technosophos/LibRIS": "dev-master"
    },
    "autoload":{
        "psr-0":{
            "Geissler":"src/"
        },
        "files": [
            "library/bibtexParse/PARSEENTRIES.php",
            "library/bibtexParse/PARSECREATORS.php",
            "library/bibtexParse/PARSEMONTH.php",
            "library/bibtexParse/PARSEPAGE.php"
        ]
    }
}
